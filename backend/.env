# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=drsara_db
DB_USER=postgres
DB_PASSWORD=postgres

# JWT Secret Key
JWT_SECRET=dr-sara-secret-key-change-in-production-2024

# Server
PORT=5000
NODE_ENV=development
FRONTEND_URL=http://localhost:3000

BASE_URL=http://localhost:5000

# MOYASAR - بوابة الدفع - سجل على: https://moyasar.com
MOYASAR_API_KEY=sk_test_YOUR_KEY_HERE
MOYASAR_PUBLISHABLE_KEY=pk_test_YOUR_KEY_HERE

# SMSA - سجل على: https://smsa.com.sa
SMSA_API_KEY=YOUR_SMSA_KEY
SMSA_PASSKEY=YOUR_SMSA_PASSKEY

# ARAMEX - سجل على: https://www.aramex.com/developers
ARAMEX_USERNAME=YOUR_EMAIL
ARAMEX_PASSWORD=YOUR_PASSWORD
ARAMEX_ACCOUNT_NUMBER=YOUR_ACCOUNT
ARAMEX_ACCOUNT_PIN=YOUR_PIN

# File Upload
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=5242880
